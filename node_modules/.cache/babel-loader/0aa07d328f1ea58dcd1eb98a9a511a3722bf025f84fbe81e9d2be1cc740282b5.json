{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TrumpQuote from \"./TrumpQuote.vue\";\nimport getDonaldTrumpQuote from \"@/service/DonaldTrumpAPI\";\nexport default {\n  name: \"TrumpQuoteGallery\",\n  components: {\n    TrumpQuote\n  },\n  data() {\n    return {\n      quotes: [],\n      sortOrder: \"asc\"\n    };\n  },\n  methods: {\n    async getTrumpQuote() {\n      const quote = await getDonaldTrumpQuote();\n      this.quotes.push(quote);\n      this.sortQuotes();\n    },\n    sortQuotes() {\n      this.quotes.sort((a, b) => a.text.localeCompare(b.text));\n      if (this.sortOrder === \"desc\") {\n        this.quotes.reverse();\n      }\n    }\n  },\n  computed: {\n    sortedQuotes() {\n      const sorted = [...this.quotes];\n      if (this.sortOrder === \"asc\") {\n        sorted.sort((a, b) => a.text.localeCompare(b.text));\n      } else {\n        sorted.sort((a, b) => b.text.localeCompare(a.text));\n      }\n      return sorted;\n    }\n  }\n};","map":{"version":3,"mappings":";AAaA,OAAOA,UAAS,MAAO,kBAAkB;AACzC,OAAOC,mBAAkB,MAAO,0BAA0B;AAE1D,eAAe;EACbC,IAAI,EAAE,mBAAmB;EAEzBC,UAAU,EAAE;IACVH;EACF,CAAC;EAEDI,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMC,aAAa,GAAG;MACpB,MAAMC,KAAI,GAAI,MAAMR,mBAAmB,EAAE;MACzC,IAAI,CAACI,MAAM,CAACK,IAAI,CAACD,KAAK,CAAC;MACvB,IAAI,CAACE,UAAU,EAAE;IACnB,CAAC;IAEDA,UAAU,GAAG;MACX,IAAI,CAACN,MAAM,CAACO,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAACE,IAAI,CAACC,aAAa,CAACF,CAAC,CAACC,IAAI,CAAC,CAAC;MACxD,IAAI,IAAI,CAACT,SAAQ,KAAM,MAAM,EAAE;QAC7B,IAAI,CAACD,MAAM,CAACY,OAAO,EAAE;MACvB;IACF;EACF,CAAC;EAEDC,QAAQ,EAAE;IACRC,YAAY,GAAG;MACb,MAAMC,MAAK,GAAI,CAAC,GAAG,IAAI,CAACf,MAAM,CAAC;MAC/B,IAAI,IAAI,CAACC,SAAQ,KAAM,KAAK,EAAE;QAC5Bc,MAAM,CAACR,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAACE,IAAI,CAACC,aAAa,CAACF,CAAC,CAACC,IAAI,CAAC,CAAC;MACrD,OAAO;QACLK,MAAM,CAACR,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACC,IAAI,CAACC,aAAa,CAACH,CAAC,CAACE,IAAI,CAAC,CAAC;MACrD;MACA,OAAOK,MAAM;IACf;EACF;AACF,CAAC","names":["TrumpQuote","getDonaldTrumpQuote","name","components","data","quotes","sortOrder","methods","getTrumpQuote","quote","push","sortQuotes","sort","a","b","text","localeCompare","reverse","computed","sortedQuotes","sorted"],"sourceRoot":"","sources":["/Users/aureliendroissart/Documents/IMAC/WEB/webproject/src/components/TrumpQuoteGallery.vue"],"sourcesContent":["<template>\n  <div class=\"quote-gallery\">\n    <h3>Welcome to the hall of shame!</h3>\n    <select v-model=\"sortOrder\" @change=\"sortQuotes\">\n      <option value=\"asc\">Sort Ascending</option>\n      <option value=\"desc\">Sort Descending</option>\n    </select>\n    <button @click=\"getTrumpQuote\">Get Quote</button>\n    <TrumpQuote v-for=\"quote in sortedQuotes\" :key=\"quote.id\" :quote=\"quote\" />\n  </div>\n</template>\n\n<script>\nimport TrumpQuote from \"./TrumpQuote.vue\";\nimport getDonaldTrumpQuote from \"@/service/DonaldTrumpAPI\";\n\nexport default {\n  name: \"TrumpQuoteGallery\",\n\n  components: {\n    TrumpQuote,\n  },\n\n  data() {\n    return {\n      quotes: [],\n      sortOrder: \"asc\",\n    };\n  },\n\n  methods: {\n    async getTrumpQuote() {\n      const quote = await getDonaldTrumpQuote();\n      this.quotes.push(quote);\n      this.sortQuotes();\n    },\n\n    sortQuotes() {\n      this.quotes.sort((a, b) => a.text.localeCompare(b.text));\n      if (this.sortOrder === \"desc\") {\n        this.quotes.reverse();\n      }\n    },\n  },\n\n  computed: {\n    sortedQuotes() {\n      const sorted = [...this.quotes];\n      if (this.sortOrder === \"asc\") {\n        sorted.sort((a, b) => a.text.localeCompare(b.text));\n      } else {\n        sorted.sort((a, b) => b.text.localeCompare(a.text));\n      }\n      return sorted;\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}