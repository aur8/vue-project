{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ChuckQuote from \"./ChuckQuote.vue\";\nimport getChuckNorrisQuote from \"@/service/ChuckNorrisAPI\";\nimport getChuckNorrisCategoryQuote from \"@/service/ChuckNorrisAPI\";\nexport default {\n  name: \"ChuckQuoteGallery\",\n  components: {\n    ChuckQuote\n  },\n  data() {\n    return {\n      quotes: []\n    };\n  },\n  methods: {\n    async getChuckQuote() {\n      const quote = await getChuckNorrisQuote();\n      this.quotes.push(quote);\n    },\n    async getChuckQuoteCategory(category) {\n      const quote = await getChuckNorrisCategoryQuote(category);\n      this.quotes.push(quote);\n    },\n    async changeCategory(selectedCategory) {\n      if (selectedCategory === \"random\") {\n        this.getChuckQuote();\n      } else {\n        this.getChuckQuoteCategory(selectedCategory);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAmBA,OAAOA,UAAS,MAAO,kBAAkB;AACzC,OAAOC,mBAAkB,MAAO,0BAA0B;AAC1D,OAAOC,2BAA0B,MAAO,0BAA0B;AAElE,eAAe;EACbC,IAAI,EAAE,mBAAmB;EAEzBC,UAAU,EAAE;IACVJ;EACF,CAAC;EAEDK,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMC,aAAa,GAAG;MACpB,MAAMC,KAAI,GAAI,MAAMR,mBAAmB,EAAE;MACzC,IAAI,CAACK,MAAM,CAACI,IAAI,CAACD,KAAK,CAAC;IACzB,CAAC;IAED,MAAME,qBAAqB,CAACC,QAAQ,EAAE;MACpC,MAAMH,KAAI,GAAI,MAAMP,2BAA2B,CAACU,QAAQ,CAAC;MACzD,IAAI,CAACN,MAAM,CAACI,IAAI,CAACD,KAAK,CAAC;IACzB,CAAC;IAED,MAAMI,cAAc,CAACC,gBAAgB,EAAE;MACrC,IAAIA,gBAAe,KAAM,QAAQ,EAAE;QACjC,IAAI,CAACN,aAAa,EAAE;MACtB,OAAO;QACL,IAAI,CAACG,qBAAqB,CAACG,gBAAgB,CAAC;MAC9C;IACF;EACF;AACF,CAAC","names":["ChuckQuote","getChuckNorrisQuote","getChuckNorrisCategoryQuote","name","components","data","quotes","methods","getChuckQuote","quote","push","getChuckQuoteCategory","category","changeCategory","selectedCategory"],"sourceRoot":"","sources":["/Users/aureliendroissart/Documents/IMAC/WEB/webproject/src/components/ChuckQuoteGallery.vue"],"sourcesContent":["<template>\n  <div class=\"quote-gallery\">\n    <h3>Welcome to the hall of the most uncommon fun facts !</h3>\n    <button @click=\"getChuckQuote\">Get Quote</button>\n    <!-- <ChuckQuote v-for=\"quote in quotes\" :key=\"quote.id\" :quote=\"quote\" /> -->\n    <select v-model=\"selectedCategory\" @change=\"changeCategory\">\n      <option value=\"random\">Random Quotes</option>\n      <option value=\"animal\">Animal Quotes</option>\n    </select>\n    <div v-if=\"selectedCategory === 'random'\">\n      <ChuckQuote v-for=\"quote in quotes\" :key=\"quote.id\" :quote=\"quote\" />\n    </div>\n    <div v-if=\"selectedCategory === 'animal'\">\n      <ChuckQuote v-for=\"quote in quotes\" :key=\"quote.id\" :quote=\"quote\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport ChuckQuote from \"./ChuckQuote.vue\";\nimport getChuckNorrisQuote from \"@/service/ChuckNorrisAPI\";\nimport getChuckNorrisCategoryQuote from \"@/service/ChuckNorrisAPI\";\n\nexport default {\n  name: \"ChuckQuoteGallery\",\n\n  components: {\n    ChuckQuote,\n  },\n\n  data() {\n    return {\n      quotes: [],\n    };\n  },\n\n  methods: {\n    async getChuckQuote() {\n      const quote = await getChuckNorrisQuote();\n      this.quotes.push(quote);\n    },\n\n    async getChuckQuoteCategory(category) {\n      const quote = await getChuckNorrisCategoryQuote(category);\n      this.quotes.push(quote);\n    },\n\n    async changeCategory(selectedCategory) {\n      if (selectedCategory === \"random\") {\n        this.getChuckQuote();\n      } else {\n        this.getChuckQuoteCategory(selectedCategory);\n      }\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}