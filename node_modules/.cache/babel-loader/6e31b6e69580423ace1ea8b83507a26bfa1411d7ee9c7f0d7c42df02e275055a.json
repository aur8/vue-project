{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TrumpQuote from \"./TrumpQuote.vue\";\nimport getDonaldTrumpQuote from \"@/service/DonaldTrumpAPI\";\nexport default {\n  name: \"TrumpQuoteGallery\",\n  components: {\n    TrumpQuote\n  },\n  data() {\n    return {\n      quotes: [],\n      sortOrder: \"asc\"\n    };\n  },\n  methods: {\n    async getTrumpQuote() {\n      const quote = await getDonaldTrumpQuote();\n      this.quotes.push(quote);\n    },\n    async sortQuotesByLength() {\n      this.quotes.sort((a, b) => {\n        const aLength = a.text.length;\n        const bLength = b.text.length;\n        if (aLength < bLength) {\n          return -1;\n        } else if (aLength > bLength) {\n          return 1;\n        } else {\n          return 0;\n        }\n      });\n      if (this.sortOrder === \"desc\") {\n        this.quotes.reverse();\n      }\n      this.sortOrder = this.sortOrder === \"asc\" ? \"desc\" : \"asc\";\n    }\n  }\n};","map":{"version":3,"mappings":";AAUA,OAAOA,UAAS,MAAO,kBAAkB;AACzC,OAAOC,mBAAkB,MAAO,0BAA0B;AAE1D,eAAe;EACbC,IAAI,EAAE,mBAAmB;EAEzBC,UAAU,EAAE;IACVH;EACF,CAAC;EAEDI,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMC,aAAa,GAAG;MACpB,MAAMC,KAAI,GAAI,MAAMR,mBAAmB,EAAE;MACzC,IAAI,CAACI,MAAM,CAACK,IAAI,CAACD,KAAK,CAAC;IACzB,CAAC;IAED,MAAME,kBAAkB,GAAG;MACzB,IAAI,CAACN,MAAM,CAACO,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACzB,MAAMC,OAAM,GAAIF,CAAC,CAACG,IAAI,CAACC,MAAM;QAC7B,MAAMC,OAAM,GAAIJ,CAAC,CAACE,IAAI,CAACC,MAAM;QAC7B,IAAIF,OAAM,GAAIG,OAAO,EAAE;UACrB,OAAO,CAAC,CAAC;QACX,OAAO,IAAIH,OAAM,GAAIG,OAAO,EAAE;UAC5B,OAAO,CAAC;QACV,OAAO;UACL,OAAO,CAAC;QACV;MACF,CAAC,CAAC;MAEF,IAAI,IAAI,CAACZ,SAAQ,KAAM,MAAM,EAAE;QAC7B,IAAI,CAACD,MAAM,CAACc,OAAO,EAAE;MACvB;MAEA,IAAI,CAACb,SAAQ,GAAI,IAAI,CAACA,SAAQ,KAAM,KAAI,GAAI,MAAK,GAAI,KAAK;IAC5D;EACF;AACF,CAAC","names":["TrumpQuote","getDonaldTrumpQuote","name","components","data","quotes","sortOrder","methods","getTrumpQuote","q